{
  "name": "phonebook",
  "version": "1.0.0",
  "description": "Phonebook exercises for FullStackOpen Part 3",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "json-server -p3001 --watch db.json",
    "build:ui": "rm -rf dist && cd ../../Part2/phonebook/ && npm run build && cp -r dist ../../Part3/phonebook",
    "deploy": "fly deploy",
    "deploy:full": "npm run build:ui && npm run deploy",
    "logs:prod": "fly logs"
  },
  "author": "Kevin C. Tofel",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "nodemon": "^3.0.1"
  }
}
